{
    "name": "element-theme",
    "version": "2.0.1",
    "description": "Theme generator cli tool for Element",
    "bin": {
        "element-theme": "./bin/element-theme",
        "et": "./bin/element-theme"
    },
    "files": [
        "bin",
        "lib"
    ],
    "main": "index.js",
    "element-theme": {
        "theme": "theme-chalk",
        "themePath": "element-plus/packages/theme-chalk"
    },
    "scripts": {
        "init": "node ./bin/element-theme -i",
        "test": "node ./bin/element-theme"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:Segma-FED/element-theme.git"
    },
    "keywords": [
        "element-ui",
        "vue",
        "theme",
        "cli"
    ],
    "bugs": {
        "url": "https://github.com/Segma-FED/element-theme/issues"
    },
    "author": "Segma FED",
    "engines": {
        "node": ">=0.12.0"
    },
    "license": "MIT",
    "peerDependencies": {
        "element-theme-chalk": "*"
    },
    "dependencies": {
        "commander": "^6.2.1",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-cssmin": "^0.2.0",
        "gulp-nop": "0.0.3",
        "gulp-sass": "^4.1.0",
        "ora": "^5.1.0",
        "run-sequence": "^2.2.1"
    },
    "devDependencies": {
        "element-plus": "^1.0.1-beta.14",
        "element-theme-chalk": "^2.0.0-alpha.1"
    }
}
